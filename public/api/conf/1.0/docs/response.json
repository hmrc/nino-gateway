  "type": "object",
  "items": {
    "properties": {
      "ninoInsightsCorrelationId": {
        "type": "string",
        "length": 36
      },
      "riskScore": {
        "description": "Risk score",
        "type": "number",
        "minimum": 0,
        "maximum": 100
      },
      "reason": {
        "description": "Reason for risk score",
        "type": "string",
        "minimum": 0,
        "maximum": 256
